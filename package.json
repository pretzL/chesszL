{
	"name": "chesszl",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev:app": "vite dev",
		"dev:websocket": "node dev-server.js",
		"dev": "concurrently \"npm run dev:app\" \"npm run dev:websocket\"",
		"build": "vite build && npm run copy-static",
		"copy-static": "cp -r static/* build/",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"concurrently": "^8.2.2",
		"sass": "^1.80.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@netlify/functions": "^2.8.2",
		"@sveltejs/adapter-netlify": "^4.3.6",
		"nanoid": "^5.0.8",
		"ws": "^8.18.0"
	}
}
